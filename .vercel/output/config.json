{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/generator$",
			"dest": "_render"
		},
		{
			"src": "^/generator/wizard$",
			"dest": "_render"
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/404/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/500/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/about/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/articles/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/articles(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/contact/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/examples/([^/]+?)/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/examples/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/examples/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/examples(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}